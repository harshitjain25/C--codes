// 1     1
// 12   21
// 123 321
// 1234321
#include <iostream>
using namespace std;
int main()
{
    int n;
    cin >> n;
    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; i <= i; j++)
        {
            cout << j;
            for (int k = 1; k <= n - 2; k++)
            {
                cout << " ";
                for (int l = i; l >= 1; l--)
                {
                    cout << l;
                }
            }
        }
    }

    return 0;
}